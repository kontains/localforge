<div class="settings-tab-content" id="web-tab" style="visibility:hidden; position:absolute; z-index:-1">
    <div class="content-scroll-area">
        <div class="settings-section compact">
            <div class="model-row-label section-title">Web Integration</div>
            <div class="settings-form-group checkbox-group compact">
                <label for="usePuppeteer">
                    <div class="checkbox-container">
                        <input type="checkbox" id="usePuppeteer">
                        <span class="custom-checkbox"></span>
                    </div>
                    <span>Use Puppeteer for web browsing</span>
                </label>
            </div>
            
            <div class="settings-form-group compact">
                <input type="text" id="googleCseId" placeholder="Google CSE ID" />
            </div>
            <div class="settings-form-group compact">
                <input type="text" id="googleApiKey" placeholder="Google API Key" />
            </div>
        </div>
    </div>
</div>